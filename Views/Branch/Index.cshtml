@{
    ViewData["Title"] = "Branch Page";
    Layout = "_CustomLayout";
}
@model List<Branch>
<section class="mt-4">
<table class="table table-hover table-border mt-4">
    <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Location</td>
            <td>City</td>
    </tr>
    
        @foreach (var item in Model)
        {
            <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Location</td>
            <td>@item.City</td>
            </tr>
        }
</table>
</section>